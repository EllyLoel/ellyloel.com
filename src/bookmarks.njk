{% set bookmarks = collections.bookmarks %}
<ul>
  {%- for bookmark in bookmarks %}
    <li>
      <a href="{{ bookmark.url | url }}">{{ bookmark.data.title }}</a>
      <small>{{ bookmark.data.page.excerpt }}</small>
    </li>
  {%- endfor %}
</ul>