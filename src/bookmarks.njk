<div id="graph"></div>

{% set bookmarks = collections.bookmarks | reverse %}
<ul class="[ flow ]">
  {%- for bookmark in bookmarks %}
    {% feedItem
    title = bookmark.data.title,
    url = bookmark.url,
    excerpt = bookmark.data.page.excerpt
 %}
  {%- endfor %}
</ul>