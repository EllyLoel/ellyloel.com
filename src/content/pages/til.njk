<dfn>Today I Learned</dfn>

<h2>
  Linked posts graph
</h2>
<div id="graph"></div>

{% set til = collections.til | sortByCreated %}
<ul class="[ feed ]">
  {%- for post in til %}
    {% feedItem
    title = post.data.title,
    url = post.url,
    excerpt = post.data.page.excerpt,
    image = post.data.image,
    created = post.data.created
 %}
  {%- endfor %}
</ul>