<div id="graph"></div>

{% set projects = collections.projects | sortByCreated %}
<ul class="[ feed ]">
  {%- for project in projects %}
    {% feedItem
    title = project.data.title,
    url = project.url,
    excerpt = project.data.page.excerpt,
    image = project.data.image,
    created = project.data.created
 %}
  {%- endfor %}
</ul>