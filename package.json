		"color-elements": "^0.0.3",
		"css-doodle": "0.42.3",
		"d3": "^7.9.0",
		"dompurify": "^3.2.5",
		"howler": "^2.2.4",
		"nude-element": "^0.0.12",
		"octokit": "^4.1.3",
		"pagefind": "^1.3.0",
		"three": "^0.172.0"
	},
