	},
	"bugs": {
		"url": "https://github.com/ellyloel/ellyloel.com/issues"
	},
	"scripts": {
		"start": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --incremental --quiet",
		"build": "pnpm build:eleventy && pnpm build:pagefind",
		"build:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --quiet",
		"build:pagefind": "npx pagefind --source _site --bundle-dir pagefind",
		"start:debug": "ELEVENTY_ENV=development DEBUG=Eleventy* npx @11ty/eleventy --serve --incremental",
		"build:debug": "ELEVENTY_ENV=production DEBUG=Eleventy* npx @11ty/eleventy",
		"build:benchmark": "ELEVENTY_ENV=production DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"clean": "rimraf _site",
		"format": "prettier . --write",
		"lint:css": "stylelint \"src/input/css/**/*.css\" --fix",
		"lint:js": "eslint src/input/js/ --fix",
		"deploy": "netlify deploy -p --build"
	},
	"packageManager": "pnpm@8.6.12",
	"dependencies": {
